    "prettier": "^2.8.2",
    "tsup": "^6.5.0",
    "type-fest": "^3.5.1",
    "typescript": "^4.9.4",
    "vitest": "^0.27.1"
  },
  "engines": {
    "node": ">=14.16"
  },
